@model IEnumerable<CoffeeShop.Models.Product>

    <!DOCTYPE html>

    <html>
    <head>
        <title>Coffe Shop</title>
    </head>
    <body>
    <h1>Coffe Shop</h1>
        @foreach (var item in Model)
    {
        <img height="100" width="100" src="@item.ImageUrl"/>
        <h3> @item.Name</h3>
        <p>@item.Price.ToString("zł")</p>
        
    }
   

    </body>

    </html>